@model Microsoft.Store.PartnerCenter.Samples.SDK.Explorer.Models.CreatedCustomerModel

@{
    Layout = null;
}

<div class="modal-header">
    <h4 class="modal-title">New Customer Successfully Created</h4>
</div>
<div class="row">
    &nbsp;
</div>
<div class="row">
    <div class="col-xs-3 col-xs-offset-1">
        @Html.LabelFor(item => Model.Username)
        @(string.Format("{0}@{1}", Model.Username, Model.Domain))
    </div>
</div>
<div class="row">
    <div class="col-xs-3 col-xs-offset-1">
        @Html.LabelFor(item => Model.Password)
        @(Model.Password)
    </div>
</div>